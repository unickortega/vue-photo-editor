{
  "name": "unick-vue-photo-editor",
  "version": "1.0.0",
  "description": "",
  "main": "dist/vue-photo-editor.umd.js",
  "module": "dist/vue-photo-editor.esm.js",
  "unpkg": "dist/vue-photo-editor.min.js",
  "browser": {
    "./sfc": "src/vue-photo-editor.vue"
  },
  "scripts": {
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-photo-editor.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-photo-editor.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-photo-editor.min.js"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "bootstrap": "^4.1.1"
  },
  "devDependencies": {
    "minimist": "^1.2.0",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^3.0.0",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unickortega/vue-photo-editor.git"
  }
}
